<h1>Listing service_agreements</h1>

<table>
  <tr>
    <th>Student</th>
    <th>Service type</th>
    <th>Class year</th>
    <th>Academic year semester</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @service_agreements.each do |service_agreement| %>
  <tr>
    <td><%= service_agreement.student_id %></td>
    <td><%= service_agreement.service_type_id %></td>
    <td><%= service_agreement.class_year_id %></td>
    <td><%= service_agreement.academic_year_semester_id %></td>
    <td><%= link_to 'Show', service_agreement %></td>
    <td><%= link_to 'Edit', edit_service_agreement_path(service_agreement) %></td>
    <td><%= link_to 'Destroy', service_agreement, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

