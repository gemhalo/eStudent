
<h1>Listing programs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Department</th>
    <th>Admission</th>
    <th>Duration</th>
    <th>Total credit hour</th>
    <th>Award type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @programs.each do |program| %>
  <tr>
    <td><%= program.name %></td>
    <td><%= program.department_id %></td>
    <td><%= program.admission.name %></td>
    <td><%= program.duration %></td>
    <td><%= program.total_credit_hour %></td>
    <td><%= program.award_type_id %></td>
    <td><%= link_to 'Show', program %></td>
    <td><%= link_to 'Edit', edit_program_path(program) %></td>
    <td><%= link_to 'Destroy', program, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Program', new_program_path %>
