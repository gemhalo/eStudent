<br>
    <div id="notification_area"  class="x-hide-display">
  <% @menuitems = menus %>
  <% if @menuitems !=nil %>
  <% @menuitems.each do |t| %>
  <% @catagory1 = t.role_id %>
  <% end %>
  <% if @catagory1 == "student_service_staff" %>
        <div class='menu_link' style="text-indent:10px"><img src='/icons/note.png'/ width=20 height=20>
          <%= link_to "Screening", student_service_staffs_selected_path %>(<%= @applicants.count unless @applicants == nil %>) <br/>
  <%end%>


  <% if @catagory1 == "student" %>
        <div class='menu_link' style="text-indent:10px"><img src='/icons/note.png'/ width=20 height=20>
          <%= link_to "Application Deadline", calendar_path %>(<%= @applicants.count unless @applicants == nil %>) <br/>
  <%end%>


  <% if @catagory1 == "instructor" %>
        <div class='menu_link' style="text-indent:10px"><img src='/icons/note.png'/ width=20 height=20>
          <%= link_to "Approve Applicants", :controller=>'admission_approval', :action=>'index' %>(<%= @applicants.count unless @applicants == nil %>) <br/>
  <% end%>


  <% if @catagory1 == "admin" %>
        <div class='menu_link' style="text-indent:10px"><img src='/icons/note.png'/ width=20 height=20>
          <%= link_to "Account Verification", :controller=>'admin', :action=>'index'%>(<%= @applicants.count unless @applicants == nil %>) <br/>
  <% end%>
 <% if @catagory1 == "guest" %>
	  <div class='menu_link' style="text-indent:10px"><img src='/icons/note.png'/ width=20 height=20>
          <%= link_to "Application Deadline", calendar_path %><%= @applicants.count unless @applicants == nil %> <br/>
  <% end%>

<% else%>
        <div class='menu_link' style="text-indent:10px"><img src='/icons/note.png'/ width=20 height=20>
          <%= link_to "Messages (0)", :controller=>'admin', :action=>'index'%> <br/>

 <% end %>
